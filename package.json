{
  "name": "@codingcatdev/sanity-plugin-podcast-rss",
  "version": "1.0.0",
  "description": "Sanity.io Studio Plugin for consuming a podcast RSS feed",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "license": "MIT",
  "author": "Alex Patterson <alex@codingcat.dev>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^2.1.14",
    "react-icons": "^5.2.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^6.8.18",
    "@sanity/plugin-kit": "^4.0.16",
    "@types/react": "^18.3.3",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.0",
    "prettier-plugin-packagejson": "^2.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "^3.44.0",
    "styled-components": "^6.1.11",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3",
    "styled-components": "^5.0 || ^6.0"
  },
  "engines": {
    "node": ">=18"
  }
}